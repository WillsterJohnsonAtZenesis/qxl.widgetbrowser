{
   "environment": {
	 "qx.icontheme": ["Tango", "Oxygen"]
   },
	"applications": [
		{
			"class": "qxl.widgetbrowser.Application",
			"theme": "qx.theme.Indigo",
			"name": "widgetbrowser",
			"title": "WidgetBrowser",
            "include" : [ 
			    "qx.theme.Modern",
 			    "qx.theme.Simple",
 			    "qx.theme.Classic",
				"osparc.theme.OSparcBlue",
				"osparc.theme.OSparcDark",
				"sqv.theme.Clean"
			], 
			"parts" :
			{
			  "boot" :       {"include" : [ "qxl.widgetbrowser.Application", "qx.theme.Indigo" ]},
 			  "tree" :       {"include" : [ "qxl.widgetbrowser.pages.Tree" ]},
			  "list" :       {"include" : [ "qxl.widgetbrowser.pages.List" ]},
 			  "table" :      {"include" : [ "qxl.widgetbrowser.pages.Table" ]},
 			  "form" :       {"include" : [ "qxl.widgetbrowser.pages.Form" ]},
 			  "toolbar":     {"include" : [ "qxl.widgetbrowser.pages.ToolBar" ]},
 			  "window" :     {"include" : [ "qxl.widgetbrowser.pages.Window" ]},
 			  "tab" :        {"include" : [ "qxl.widgetbrowser.pages.Tab" ]},
 			  "control":     {"include" : [ "qxl.widgetbrowser.pages.Control" ]},
 			  "embed" :      {"include" : [ "qxl.widgetbrowser.pages.Embed" ]},
 			  "embedframe" : {"include" : [ "qxl.widgetbrowser.pages.EmbedFrame" ]},
 			  "basic" :      {"include" : [ "qxl.widgetbrowser.pages.Basic" ]},
 			  "misc" :   	 {"include" : [ "qxl.widgetbrowser.pages.Misc" ]}
			}
		}
	],

	/** Targets */
	"targets": [
		{
			"type": "source",
			"outputPath": "source-output"
		},
		{
			"type": "build",
			"outputPath": "build-output"
		}
	],
	
	"defaultTarget": "source",
	
	"locales": [
		"en"
	],
	"serve": {
	   "listenPort": 8080
	}
}